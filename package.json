{
  "name": "langchain-app",
  "version": "1.0.0",
  "description": "Langchain app using Google GenAI and TypeScript",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx index.ts",
    "build": "tsc",
    "watch": "tsc -w",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "docker:app": "docker-compose up node_app_dev",
    "docker:app:down": "docker-compose down node_app_dev",
    "docker:app:build": "docker-compose up --build node_app_dev",
    "docker:app:logs": "docker-compose logs -f node_app_dev",
    "docker:clean": "docker-compose down && docker network prune -f && docker volume prune -f",
    "docker:up": "docker-compose up -d redis postgres pgadmin evolution_v2 redis_app",
    "docker:down": "docker-compose down redis postgres pgadmin evolution_v2 redis_app node_app_dev ",
    "docker:up:prod": "docker-compose up -d redis postgres pgadmin evolution_v2 redis_app node_app",
    "docker:networks": "docker network ls"
  },
  "dependencies": {
    "@langchain/core": "^0.3.54",
    "@langchain/google-genai": "^0.2.6",
    "@langchain/groq": "^0.2.2",
    "@langchain/langgraph": "^0.2.68",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "ioredis": "^5.6.1",
    "openai": "^4.98.0",
    "pino": "^8.19.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.5",
    "@types/pino": "^7.0.5",
    "@types/redis": "^4.0.11",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  }
}
